<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>


    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <script src="http://code.jquery.com/jquery.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" ></script>
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/css/AdminLTE.css" rel="stylesheet" />
    <link href="~/Content/css/_all-skins.min.css" rel="stylesheet" />
    <link href="~/Content/css/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/css/datepicker3.css" rel="stylesheet" />

    <link href="~/Content/css/style.css" rel="stylesheet" />

    <script src="~/Content/js/angular.min.js"></script>
    <script src="~/Content/js/bootstrap-datepicker.js"></script>
    <script src="~/Content/js/bootstrap-table.js"></script>
    <script src="~/Content/js/chart-data.js"></script>
    <script src="~/Content/js/chart.min.js"></script>
    <script src="~/Content/js/ckeditor.js"></script>
    <script src="~/Content/js/dashboard.js"></script>
    <script src="~/Content/js/easypiechart-data.js"></script>
    <script src="~/Content/js/easypiechart.js"></script>
    <script src="~/Content/js/function.js"></script>
    <script src="~/Content/js/html5shiv.min.js"></script>
    <script src="~/Content/js/jquery-ui.js"></script>
    <script src="~/Content/js/jquery.nestable.js"></script>
    <script src="~/Content/js/lumino.glyphs.js"></script>
    <script src="~/Content/js/respond.min.js"></script>

    <script src="~/Content/js/ajax.js"></script>
    <script src="~/Content/js/app.js"></script>


</head>
<body class="hold-transition skin-blue sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">
        <!-- Chèn header layout -->
        <header class="main-header">
            <!-- Logo -->

            <a href="" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Administrator</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <ul class="nav navbar-nav">
                    <li><a href="" style="font-size: 1.5em;">Hôm nay là ngày: @DateTime.Now.Day/@DateTime.Now.Month/@DateTime.Now.Year</a></li></h4 >
                </ul>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle user-login" data-toggle="dropdown">Xin chào MT <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a href=""><span class="glyphicon glyphicon-off"></span> Đăng xuất</a></li>
                            </ul>
                        </li>
                        <li><a href="" title="">Đăng nhập</a></li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/lte/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>Mặt Than</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">MAIN NAVIGATION</li>
                    <!-- /.thống kê -->
                    <li class="">
                        <a href="">
                            <i class="fa fa-home"></i> <span>Trang chủ</span>
                        </a>
                    </li>
                    <!-- NSX -->
                    <li class="">
                        <a href="">
                            <i class="glyphicon glyphicon-cog"></i> <span>Nhà sản xuất</span>
                        </a>
                    </li>
                    <!-- NCC -->
                    <li class="">
                        <a href="">
                            <i class="glyphicon glyphicon-save"></i> <span>Nhà cung cấp</span>
                        </a>
                    </li>
                    <!-- /.Categorys -->
                    <li class="">
                        <a href="">
                            <i class="glyphicon glyphicon-th-list"></i> <span>Danh mục</span>
                        </a>
                    </li>
                    <!-- /.Products -->
                    <li class="">
                        <a href="">
                            <i class="fa fa-folder"></i> <span>Sản phẩm</span>
                        </a>
                    </li>
                    <!-- Nhap hang -->
                    <li>
                        <a href="">
                            <i class="glyphicon glyphicon-import"></i> <span>Nhập hàng</span>
                        </a>
                    </li>
                    <!-- /.Orders -->
                    <li>
                        <a href="">
                            <i class="glyphicon glyphicon-shopping-cart"></i> <span>Đơn hàng</span>
                        </a>
                    </li>
                    <!-- / .User backend-->
                    <li>
                        <a href="">
                            <i class="fa fa-user"></i> <span>Tài khoản</span>
                        </a>
                    </li>
                    <!-- / .Banner-->
                    <li>
                        <a href="">
                            <i class="fa fa-user"></i> <span>Banner</span>
                        </a>
                    </li>
                    <!-- / .News-->
                    <li>
                        <a href="">
                            <i class="fa fa-user"></i> <span>Tin tức</span>
                        </a>
                    </li>

                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    <!-- Tiêu đề  -->
                    @RenderSection("title", required: false)
                </h1>
            </section>
            <section class="content">

                <!-- Default box -->
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">@RenderSection("box-title", required: false)</h3>

                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                                    title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                                <i class="fa fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        @RenderBody()
                        <hr />
                        <footer>
                            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
                        </footer>
                    </div>
                </div>
                <!-- /.box-body -->
                <!-- /.box -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

        <footer class="main-footer" style="height: 50px;">
            <div class="pull-right hidden-xs" style="padding-bottom: 10px;">
                <p><b>Version</b> 2.4.0</p>
            </div>

        </footer>

    </div>
</body>
</html>
